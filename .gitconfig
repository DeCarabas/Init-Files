[user]
	name = John Doty
	email = jddoty@gmail.com
[core]
	editor = c:/chocolatey/bin/emacs.bat
	autocrlf = true
[pretty]
	doty = * %C(bold green)%h%Creset -%C(bold white)%d%Creset %s %C(bold)(%cr)%Creset <%aN>%Creset
[format]
	pretty = doty
[alias]
    st = status
    ci = commit
[push]
	default = upstream
